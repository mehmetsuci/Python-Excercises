num = int(input('Enter a positive number to check if it is a prime number or not? :'))
divisors = []
for i in range(1, num + 1):
    if num % i == 0:
        divisors.append(i)
#print(divisors)
if divisors == [1, num]:
    print(f'{num} is a prime number')
else:
    print(f'{num} is not a prime number')